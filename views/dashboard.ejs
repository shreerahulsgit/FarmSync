            const question = button.closest('.accordion-item').querySelector('.accordion-button span:first-child').textContent;

            try {
                const response = await fetch('/api/feedback', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({ question, feedback })
                });

                const data = await response.json();
                if (response.ok) {
                    alert(`Thank you for your feedback! Submission ID: ${data.feedbackId}`);
                } else {
                    alert('Failed to submit feedback. Please try again.');
                }
            } catch (error) {
                console.error('Error submitting feedback:', error);
                alert('An error occurred. Please contact support.');
            }
        });
    });
</script>

